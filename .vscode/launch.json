{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "cortex-debug",
            "request": "launch",
            "name": "Debug (J-Link)",
            "cwd": "${workspaceRoot}",
            "executable": "./build/ch.elf",
            "servertype": "jlink",
            "device": "STM32F411RE",
            "gdbTarget": "localhost:50000",
            "interface": "swd",
            "serialNumber": "",
            "runToMain": true,
            "swoConfig": {
                "enabled": true,
                "cpuFrequency": 96000000,
                "swoFrequency": 2000000,
                "source": "probe",
                "decoders": [
                    { 
                        "type": "console",
                        "label": "ITM", 
                        "port": 0,
                        "showOnStartup": true,
                        "encoding": "ascii"
                    }
                ]
            }
        },
    ]
}